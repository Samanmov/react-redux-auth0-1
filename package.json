    "name": "rps",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "auth0-js": "^8.10.1",
        "history": "^4.7.2",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-redux": "^5.0.0-alpha.8",
