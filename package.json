        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0"
    },
    "scripts": {
        "start":
            "REACT_APP_AUTH0_DOMAIN=$AUTH0_DOMAIN REACT_APP_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID react-scripts start",
